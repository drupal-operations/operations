<?php

/**
 * @file
 * Install, update and uninstall functions for the Site module.
 */

/**
 * Implements hook_install().
 */
function site_install() {
  \Drupal\site\Entity\SiteDefinition::load('self')->saveEntity(t('Initial site entity created on installation of site.module.'));
}

/**
 * Implements hook_uninstall().
 */
function site_uninstall() {
}
